{
  "title": "Test cAdvisor",
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Test Count",
      "targets": [
        {
          "expr": "count(container_cpu_usage_seconds_total)"
        }
      ],
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "CPU All",
      "targets": [
        {
          "expr": "rate(container_cpu_usage_seconds_total[5m])",
          "legendFormat": "{{id}}"
        }
      ],
      "gridPos": {"h": 8, "w": 18, "x": 6, "y": 0}
    }
  ],
  "time": {"from": "now-1h", "to": "now"},
  "refresh": "10s"
}
